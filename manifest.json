{
  "manifest_version": 3,
  "name": "QuickBooks Time Manager",
  "version": "2.0.1",
  "_comment_version": "DO NOT UPDATE VERSION HERE - Update version in package.json instead. It syncs automatically via 'npm run sync-version'",
  "description": "Manage QuickBooks Time data from a Chrome extension",
  "icons": {
    "16": "images/hourglass_icon_16.png",
    "48": "images/hourglass_icon_48.png",
    "128": "images/hourglass_icon_128.png"
  },
  "permissions": [
    "identity",
    "storage",
    "activeTab",
    "alarms",
    "offscreen",
    "notifications"
  ],
  "host_permissions": [
    "https://rest.tsheets.com/api/*"
  ],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background/background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "sounds/*.mp3"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
